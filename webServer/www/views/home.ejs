	<!-- write the header -->
	<% include partials/header %>

	<div class="container wrapper">
		<div class="row">
			<div class="col-md-8 col-md-offset-1"><h1 style="font-weight:bold; color: #FA7470; font-size: 75px">Stylize</h1></div>
		</div>
		<div class="row">
			<div class="col-md-10 col-md-offset-1"><h1 style="margin-top: 10px">Upload a Photo or Video to Style</h1></div>
		</div>	
		<div class="row">
			<div class="col-md-4 col-md-offset-1 upload" id="upload-photo">
		      	<div class="file-upload" data-toggle="modal" data-target="#uploadModal">
		        	<div class="file-select">	
		          		<div class="file-select-button" id="fileName"><i class="material-icons" id="camera_icon" style="font-size: 30px">camera_alt</i></div>
		          		<div class="file-select-name" id="noFile" style="font-size:20px">Choose Photo</div>
		          		<form method="post" id="uploadPhotoForm" enctype="multipart/form-data">
		          			<input id="uploadPhoto" type="file" class="upload" accept=".png,.jpg" name="content"   />
		          		</form>	
		        	</div>
		      	</div>
			</div>
			<div class="col-md-4 upload" id="upload-video">   	
				<div class="file-upload" data-toggle="modal" data-target="#myModal">
		        	<div class="file-select">
		          		<div class="file-select-button" id="fileName"><i class="material-icons" id="video_icon" style="font-size: 30px">videocam</i></div>
		          		<div class="file-select-name" id="noFile" style="font-size:20px">Choose Video</div>
		          		<form method="post" id="uploadVideoForm" enctype="multipart/form-data">
		          			<input id="uploadVideo" type="file" class="upload" multiple="multiple" accept="image/*" name="content" />
		          		</form>	
		        	</div>
		      	</div>
			</div>	
		</div>
